<?xml version="1.0"?>
<robot name="track" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Define properties based on your requirements -->
  <xacro:property name="chassis_width" value="0.355"/> <!-- 35.5 cm -->
  <xacro:property name="total_width" value="0.695"/> <!-- 69.5 cm -->
  <xacro:property name="track_width" value="0.165"/> <!-- 16.5 cm -->
  <xacro:property name="vehicle_length" value="1.285"/> <!-- 128.5 cm -->
  <xacro:property name="vehicle_height" value="0.575"/> <!-- 57.5 cm -->
  
  <!-- Calculated dimensions -->
  <xacro:property name="track_length" value="${vehicle_length * 0.9}"/> <!-- 1.1565 m -->
  <xacro:property name="chassis_height" value="${vehicle_height * 0.6}"/> <!-- 0.345 m -->
  <xacro:property name="wheel_radius" value="${vehicle_height * 0.3}"/> <!-- 0.1725 m -->
  <xacro:property name="wheel_sep_length" value="${track_length * 0.7}"/> <!-- 0.80955 m -->
  
  <!-- Additional calculated values for positioning -->
  <xacro:property name="track_offset_y" value="${(total_width - track_width) / 2}"/> <!-- 0.265 m from center -->
  
  <!-- ENHANCED FLIPPER DIMENSIONS -->
  <xacro:property name="flipper_wheel_radius" value="${wheel_radius * 0.75}"/> <!-- Increased from 0.5 to 0.75 -->
  <xacro:property name="flipper_small_wheel_radius" value="${flipper_wheel_radius * 0.4}"/> <!-- Proportionally larger -->
  <xacro:property name="flipper_length" value="0.35"/> <!-- Increased from 0.33 to 0.50 -->
  <xacro:property name="flipper_width" value="${track_width * 0.6}"/> <!-- Increased from 0.3 to 0.4 -->
  <xacro:property name="flipper_bar_height" value="0.075"/> <!-- Increased from 0.055 to 0.075 -->
  <xacro:property name="flipper_middle_bar_width" value="0.28"/> <!-- Increased from 0.2 to 0.28 -->
  <xacro:property name="flipper_middle_bar_height" value="0.09"/> <!-- Increased from 0.07 to 0.09 -->

  <!-- Base Footprint (Ground Plane Reference) -->
  <!-- <link name="base_footprint">
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="0.0001"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" 
               iyy="0.0001" iyz="0.0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link> -->

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.010" rpy="0 0 0"/>
  </joint>

  <!-- Base Link (Chassis) -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 ${chassis_height/2}" rpy="0 0 0"/>
      <mass value="13.14"/>
      <inertia ixx="0.10019" ixy="0" ixz="0" 
               iyy="0.345043" iyz="0" izz="0.302044"/>
    </inertial>
    <collision>
      <origin xyz="0 0 ${chassis_height/2}" rpy="0 0 0"/>
      <geometry>
        <box size="${vehicle_length} ${chassis_width} ${chassis_height}"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 ${chassis_height/2}" rpy="0 0 0"/>
      <geometry>
        <box size="${vehicle_length} ${chassis_width} ${chassis_height}"/>
      </geometry>
    </visual>
  </link>


  <!-- Left Track -->
  <link name="left_track">
    <inertial>
      <origin xyz="0 0 0.0141" rpy="0 0 0"/>
      <mass value="6.06"/>
      <inertia ixx="0.002731" ixy="0" ixz="0" 
               iyy="0.032554" iyz="0.000015" izz="0.031391"/>
    </inertial>
    <collision name="left_track_collision">
      <origin xyz="0 0 ${wheel_radius/2}" rpy="1.5708 0 1.5708"/>
      <geometry>
        <box size="${track_width} ${wheel_radius} ${track_length}"/>
      </geometry>
    </collision>
    <collision name="left_track_collision_1">
      <origin xyz="${wheel_sep_length/2} 0 ${wheel_radius/2}" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="${track_width}" radius="${wheel_radius/2}"/>
      </geometry>
    </collision>
    <collision name="left_track_collision_2">
      <origin xyz="${-wheel_sep_length/2} 0 ${wheel_radius/2}" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="${track_width}" radius="${wheel_radius/2}"/>
      </geometry>
    </collision>
    <visual name="left_track_visual">
      <origin xyz="0 0 ${wheel_radius/2}" rpy="1.5708 0 1.5708"/>
      <geometry>
        <box size="${track_width} ${wheel_radius} ${track_length}"/>
      </geometry>
    </visual>
    <visual name="left_track_visual_1">
      <origin xyz="${wheel_sep_length/2} 0 ${wheel_radius/2}" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="${track_width}" radius="${wheel_radius/2}"/>
      </geometry>
    </visual>
    <visual name="left_track_visual_2">
      <origin xyz="${-wheel_sep_length/2} 0 ${wheel_radius/2}" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="${track_width}" radius="${wheel_radius/2}"/>
      </geometry>
    </visual>
  </link>

  <joint name="left_track_j" type="revolute">
    <parent link="base_link"/>
    <child link="left_track"/>
    <origin xyz="0 ${track_offset_y} 0" rpy="0 0 0"/>
    <limit lower="0" upper="0" effort="1000" velocity="0"/>
    <dynamics damping="1000" friction="1000"/>
  </joint>

