      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
  </link>

  <joint name="velodyne_joint" type="fixed">
    <parent link="base_link"/>
    <child link="velodyne"/>
    <origin xyz="0.63 0 ${chassis_height + 0.025}" rpy="0 0 0"/>
  </joint>

  <!-- Gazebo-specific elements -->
  <gazebo reference="base_link">
    <sensor name="imu_sensor" type="imu">
      <always_on>1</always_on>
      <update_rate>1</update_rate>
      <visualize>true</visualize>
      <topic>imu</topic>
    </sensor>
  </gazebo>

  <gazebo reference="left_track">
    <mu1>0.7</mu1>
    <mu2>150</mu2>
    <fdir1>0 1 0</fdir1>
  </gazebo>

  <gazebo reference="right_track">
    <mu1>0.7</mu1>
    <mu2>150</mu2>
    <fdir1>0 1 0</fdir1>
  </gazebo>

  <gazebo reference="front_left_flipper">
    <mu1>0.7</mu1>
    <mu2>150</mu2>
    <fdir1>0 1 0</fdir1>
    <selfCollide>true</selfCollide>
    <maxContacts>20</maxContacts>
  </gazebo>

  <gazebo reference="rear_left_flipper">
    <mu1>0.7</mu1>
    <mu2>150</mu2>
    <fdir1>0 1 0</fdir1>
    <selfCollide>true</selfCollide>
    <maxContacts>20</maxContacts>
  </gazebo>

  <gazebo reference="front_right_flipper">
    <mu1>0.7</mu1>
    <mu2>150</mu2>
    <fdir1>0 1 0</fdir1>
    <selfCollide>true</selfCollide>
    <maxContacts>20</maxContacts>
  </gazebo>

  <gazebo reference="rear_right_flipper">
    <mu1>0.7</mu1>
    <mu2>150</mu2>
    <fdir1>0 1 0</fdir1>
    <selfCollide>true</selfCollide>
    <maxContacts>20</maxContacts>
  </gazebo>

  <gazebo reference="velodyne">
    <material>Gazebo/Blue</material>
    <sensor name="velodyne" type="gpu_lidar">
      <update_rate>10</update_rate>
      <visualize>true</visualize>
      <always_on>true</always_on>
      <gz_frame_id>velodyne</gz_frame_id>
      <topic>velodyne</topic>
      <lidar>
        <scan>
          <horizontal>
            <samples>2048</samples>
            <resolution>1</resolution>
            <min_angle>-3.14159265359</min_angle>
            <max_angle>3.14159265359</max_angle>
          </horizontal>
          <vertical>
            <samples>128</samples>
            <resolution>1</resolution>
            <min_angle>-0.55</min_angle>
            <max_angle>0.26</max_angle>
          </vertical>
        </scan>
        <range>
          <min>0.75</min>
          <max>131.0</max>
          <resolution>0.01</resolution>
        </range>
      </lidar>
    </sensor>
  </gazebo>

  <link name="rplidar">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" 
               iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.04"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.04"/>
      </geometry>
      <material name="black">
        <color rgba="0.2 0.2 0.2 1"/>
      </material>
    </visual>
  </link>

  <joint name="rplidar_joint" type="fixed">
    <parent link="velodyne"/>
    <child link="rplidar"/>
    <!-- Positioned on top of Velodyne (0.025 is half of Velodyne height + 0.02 half of RP LiDAR height + small gap) -->
    <origin xyz="0 0 0.045" rpy="0 0 0"/>
  </joint>

  <!-- Gazebo RP LIDAR Plugin -->
  <gazebo reference="rplidar">
    <material>Gazebo/Black</material>
    <sensor name="rplidar" type="gpu_lidar">
      <always_on>true</always_on>
      <visualize>false</visualize>
      <update_rate>10</update_rate>
      <topic>scan</topic>
      <gz_frame_id>velodyne</gz_frame_id>
      <lidar>
        <scan>
          <horizontal>
            <samples>360</samples>
            <resolution>1.000000</resolution>
            <min_angle>-3.14159</min_angle>
            <max_angle>3.14159</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.75</min>
          <max>15.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </lidar>
    </sensor>
  </gazebo>

  <!-- Gazebo Plugins -->
  <gazebo>
    <plugin name="gz::sim::systems::TrackedVehicle" filename="gz-sim-tracked-vehicle-system">
      <left_track>
        <link>left_track</link>
      </left_track>
      <left_track>
        <link>front_left_flipper</link>
      </left_track>
      <left_track>
        <link>rear_left_flipper</link>
      </left_track>
      <right_track>
        <link>right_track</link>
      </right_track>
      <right_track>
        <link>front_right_flipper</link>
      </right_track>
      <right_track>
        <link>rear_right_flipper</link>
      </right_track>
      <topic>cmd_vel</topic>
      <tracks_separation>${track_offset_y * 2}</tracks_separation>
      <tracks_height>${wheel_radius}</tracks_height>
      <steering_efficiency>0.7</steering_efficiency>
      <odom_publish_frequency>50</odom_publish_frequency>
      <odom_topic>/odom</odom_topic>
      <tf_topic>/tf</tf_topic>
      <frame_id>odom</frame_id>
      <child_frame_id>base_footprint</child_frame_id>
    </plugin>

    <plugin name="gz::sim::systems::TrackController" filename="gz-sim-track-controller-system">
      <link>left_track</link>
      <min_velocity>-1.0</min_velocity>
      <max_velocity>3.0</max_velocity>
    </plugin>

    <plugin name="gz::sim::systems::TrackController" filename="gz-sim-track-controller-system">
      <link>right_track</link>
      <min_velocity>-1.0</min_velocity>
      <max_velocity>3.0</max_velocity>
    </plugin>

    <plugin name="gz::sim::systems::TrackController" filename="gz-sim-track-controller-system">
      <link>front_left_flipper</link>
      <min_velocity>-1.0</min_velocity>
      <max_velocity>3.0</max_velocity>
    </plugin>

    <plugin name="gz::sim::systems::TrackController" filename="gz-sim-track-controller-system">
      <link>rear_left_flipper</link>
      <min_velocity>-1.0</min_velocity>
      <max_velocity>3.0</max_velocity>
    </plugin>

    <plugin name="gz::sim::systems::TrackController" filename="gz-sim-track-controller-system">
      <link>front_right_flipper</link>
      <min_velocity>-1.0</min_velocity>
      <max_velocity>3.0</max_velocity>
    </plugin>

    <plugin name="gz::sim::systems::TrackController" filename="gz-sim-track-controller-system">
      <link>rear_right_flipper</link>
      <min_velocity>-1.0</min_velocity>
      <max_velocity>3.0</max_velocity>
    </plugin>

    <plugin filename="gz-sim-joint-state-publisher-system" name="gz::sim::systems::JointStatePublisher">
      <topic>joint_states</topic>
      <update_rate>30</update_rate>
      <!-- <joint_name>base_footprint_joint</joint_name> -->
      <joint_name>left_track_j</joint_name>
      <joint_name>right_track_j</joint_name>
      <joint_name>front_left_flipper_j</joint_name>
      <joint_name>front_right_flipper_j</joint_name>
      <joint_name>rear_left_flipper_j</joint_name>
      <joint_name>rear_right_flipper_j</joint_name>
    </plugin>
  </gazebo>

</robot>